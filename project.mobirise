              "background-color": "@container",
              "@media (max-width: 992px)": {
                "padding": "85px 8px",
                "margin": "0"
              },
              ".item": {
                "padding": "0 12px",
                "z-index": "1",
                "position": "relative"
              },
              ".decor-wrapper": {
                "position": "absolute",
                "bottom": "0",
                "right": "-10rem",
                "height": "100%",
                "width": "100%",
                "display": "flex",
                "flex-direction": "column",
                "justify-content": "flex-end",
                "align-items": "flex-end",
                "padding-bottom": "20%",
                "@media (max-width: 992px)": {
                  "display": "none"
                },
                ".decor": {
                  "border": "0.5px solid @decor",
                  "border-right": "none",
                  "border-radius": "10px 0 0 10px",
                  "height": "190px",
                  "width": "30%",
                  "margin-bottom": "60px",
                  "&:first-child": {
                    "width": "40%"
                  },
                  "&:last-child": {
                    "width": "45%",
                    "margin-bottom": "0"
                  }
                }
              }
            },
            ".title-wrapper": {
              "position": "relative",
              "z-index": "1",
              ".mbr-section-title": {
                "margin-bottom": "85px",
                "@media (max-width: 992px)": {
                  "margin-bottom": "50px"
                }
              }
            },
            ".item": {
              "margin-bottom": "85px",
              "@media (max-width: 992px)": {
                "margin-bottom": "50px"
              },
              ".item-wrapper": {
                "overflow": "hidden",
                "border-radius": "10px",
                "border": "1px solid @card",
                ".item-img": {
                  "position": "relative",
                  "min-height": "470px",
                  "@media (max-width: 768px)": {
