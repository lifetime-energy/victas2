                  },
                  "img": {
                    "position": "absolute",
                    "top": "0",
                    "left": "0",
                    "width": "100%",
                    "height": "100%",
                    "object-fit": "cover"
                  },
                  ".desc-wrap": {
                    "position": "absolute",
                    "top": "0",
                    "right": "0",
                    "width": "100%",
                    ".mbr-desc": {
                      "display": "inline-flex",
                      "padding": "8px 15px",
